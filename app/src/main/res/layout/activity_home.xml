<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_home"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <TextView
            android:id="@+id/textView13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginStart="8dp"
            android:layout_marginTop="32dp"
            android:layout_marginEnd="8dp"
            android:fontFamily="serif"
            android:text="Diabetes Tracker"
            android:textColor="#8c2020"
            android:textSize="30sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/button7"
            style="@android:style/Widget.Holo.Button.Small"
            android:layout_width="120dp"
            android:layout_height="100dp"
            android:layout_below="@+id/button6"
            android:layout_alignStart="@+id/button6"
            android:layout_alignLeft="@+id/button6"
            android:layout_gravity="end"
            android:layout_marginTop="62dp"
            android:onClick="inquiry"
            android:text="Inquiry"
            android:textAlignment="textEnd"
            android:textAllCaps="true"
            android:textSize="14sp"
            android:textStyle="bold" />

        <Button
            android:id="@+id/button8"
            style="@android:style/Widget.Holo.Button.Small"
            android:layout_width="120dp"
            android:layout_height="100dp"
            android:layout_alignTop="@+id/button7"
            android:layout_alignEnd="@+id/button3"
            android:layout_alignRight="@+id/button3"
            android:layout_gravity="end"
            android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            android:onClick="article"
            android:text="article"
            android:textAlignment="textEnd"
            android:textAllCaps="true"
            android:textSize="14sp"
            android:textStyle="bold" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="50dp"
            android:layout_height="60dp"
            android:layout_alignStart="@+id/button3"
            android:layout_alignLeft="@+id/button3"
            android:layout_alignTop="@+id/button3"
            android:layout_marginStart="11dp"
            android:layout_marginLeft="11dp"
            app:srcCompat="@drawable/diet" />

        <Button
            android:id="@+id/button3"
            style="@android:style/Widget.Holo.Button.Small"
            android:layout_width="120dp"
            android:layout_height="100dp"
            android:layout_below="@+id/textView13"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_gravity="end"
            android:layout_marginTop="100dp"
            android:layout_marginEnd="60dp"
            android:layout_marginRight="60dp"
            android:onClick="diet_program"
            android:text="Diet Program"
            android:textAlignment="textEnd"
            android:textAllCaps="true"
            android:textStyle="bold" />

        <Button
            android:id="@+id/button6"
            style="@android:style/Widget.Holo.Button.Small"
            android:layout_width="120dp"
            android:layout_height="100dp"
            android:layout_below="@+id/textView13"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_gravity="end"
            android:layout_marginTop="100dp"
            android:layout_marginEnd="230dp"
            android:layout_marginRight="230dp"
            android:onClick="schedule"
            android:text="schedule"
            android:textAlignment="textEnd"
            android:textAllCaps="true"
            android:textStyle="bold" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_alignStart="@+id/button6"
            android:layout_alignLeft="@+id/button6"
            android:layout_alignTop="@+id/button6"
            android:layout_marginStart="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="8dp"
            app:srcCompat="@drawable/schedule" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_alignStart="@+id/button3"
            android:layout_alignLeft="@+id/button3"
            android:layout_alignTop="@+id/imageView5"
            app:srcCompat="@drawable/artical" />

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_alignStart="@+id/imageView3"
            android:layout_alignLeft="@+id/imageView3"
            android:layout_alignTop="@+id/button7"
            android:layout_marginTop="12dp"
            app:srcCompat="@drawable/inquiry" />


    </RelativeLayout>

    <android.support.design.widget.NavigationView
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        app:theme="@style/NavigationDrawerStyle"
        app:menu="@menu/navigation_menu"
        android:layout_gravity="start"
        app:headerLayout="@layout/nav_header"
        android:id="@+id/nv">

    </android.support.design.widget.NavigationView>



</android.support.v4.widget.DrawerLayout>
